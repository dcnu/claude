      "Bash(cat .env.development:*)",
      "Bash(cat .env.local:*)",
      "Bash(cat .env.production:*)",
      "Bash(curl --data:*)",
      "Bash(curl -d:*)",
      "Bash(curl -X DELETE:*)",
      "Bash(curl -X PATCH:*)",
      "Bash(curl -X POST:*)",
      "Bash(curl -X PUT:*)",
      "Bash(echo $:*)",
      "Bash(env:*)",
      "Bash(head .env:*)",
      "Bash(less .env:*)",
      "Bash(more .env:*)",
      "Bash(printenv:*)",
      "Bash(prisma migrate reset:*)",
      "Bash(rm -rf:*)",
      "Bash(rm:*)",
      "Bash(rmdir:*)",
