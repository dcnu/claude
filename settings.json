    "Bash(curl -X POST:*)",
    "Bash(curl -X PUT:*)",
    "Bash(curl -X DELETE:*)",
    "Bash(curl -X PATCH:*)",
    "Bash(curl --data:*)",
    "Bash(curl -d:*)",
    "Bash(cat .env)",
    "Bash(cat .env.local:*)",
    "Bash(cat .env.production:*)",
    "Bash(cat .env.development:*)",
    "Bash(head .env:*)",
    "Bash(tail .env:*)",
    "Bash(less .env:*)",
    "Bash(more .env:*)",
    "Bash(echo $:*)",
    "Bash(printenv:*)",
    "Bash(env:*)"
  ]
}