      "Bash(prettier:*)",
      "Bash(prisma format:*)",
      "Bash(prisma generate:*)",
      "Bash(prisma validate:*)",
      "Bash(prisma studio:*)",
      "Bash(psql -c SELECT:*)",
      "Bash(psql -c \\d:*)",
      "Bash(pytest:*)",
      "Bash(python3:*)",
      "Bash(ruff:*)",
      "Bash(supabase db diff:*)",
      "Bash(supabase db dump:*)",
      "Bash(supabase gen types:*)",
      "Bash(supabase migration list:*)",
      "Bash(supabase migration new:*)",
      "Bash(supabase start:*)",
      "Bash(supabase status:*)",
